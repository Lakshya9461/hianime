{
  "id": "hianime",
  "name": "HiAnime",
  "version": "1.0.0",
  "manifestVersion": 1,
  "type": "anime",
  "language": "en",
  "author": "Community",
  "description": "HiAnime provider for Seanime",
  "homepage": "https://hianimez.is/home",
  "baseURL": "https://hianimez.is",
  "manifestURI": "https://raw.githubusercontent.com/Lakshya9461/hianime/main/manifest.json",

  "payload": {
    "type": "scraper",
    "routes": {
      "search": "/search?keyword={query}",
      "anime": "/watch/{id}",
      "episodes": "/ajax/episode/list/{id}",
      "stream": "/ajax/episode/sources/{episodeId}"
    }
  }
}
